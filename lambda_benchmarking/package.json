{
  "name": "reco-benchmark",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:function": "serverless deploy function -f benchmark",
    "benchmark": "time npx ts-node -T ./run_benchmarks.ts; open benchmarks_result/index.html",
    "benchmark:single": "time node -r ts-node/register/transpile-only --trace-deopt cli.ts"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.68",
    "@types/node": "^14.14.16",
    "@types/serverless": "^1.78.16",
    "axios": "^0.21.1",
    "changed-git-files": "^0.0.1",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "serverless": "^2.16.1",
    "serverless-plugin-export-endpoints": "^0.1.2",
    "serverless-webpack": "^5.3.5",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
