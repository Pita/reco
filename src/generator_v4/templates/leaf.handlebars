{{#unless noop}}
  {{#if lastComparison}}
    result{{{atomIndex}}} = {{>comparison atomIndex=atomIndex}}
  {{/if}}
  {{#unless lastComparison}}
    if ({{>comparison atomIndex=atomIndex}}) {
      {{>leaf comparisonTrue atomIndex=atomIndex}}
    }
    {{#if comparisonFalse}}
      else {
        {{>leaf comparisonFalse atomIndex=atomIndex}}
      }
    {{/if}}
  {{/unless}}
{{/unless}}