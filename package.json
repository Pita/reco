{
  "name": "reco",
  "version": "0.0.1",
  "description": "Regular Expression COmpiler",
  "main": "index.js",
  "scripts": {
    "test:all": "npm run test:generate && npm run test:run",
    "test:generate": "ts-node ./src/tests/generate_tests.ts",
    "test:snapshots": "npx jest --updateSnapshot",
    "test:run": "npx jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "build": "tsc --build",
    "start": "ts-node ./src/index",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pita/reco.git"
  },
  "keywords": [
    "regex",
    "compile",
    "regular",
    "expression"
  ],
  "author": "Peter 'Pita' Martischka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pita/reco/issues"
  },
  "homepage": "https://github.com/Pita/reco#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.4",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.4",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "jest": "^26.4.2",
    "mkdirp": "^1.0.4",
    "prettier": "2.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "handlebars": "^4.7.6",
    "js-string-escape": "^1.0.1",
    "lodash": "^4.17.20",
    "regexp-to-ast": "^0.5.0"
  }
}
