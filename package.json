{
  "name": "reco.io",
  "version": "0.1.2",
  "description": "Regular Expression COmpiler",
  "main": "index.js",
  "scripts": {
    "test:all": "npm run generate && npm run test:run",
    "generate": "ts-node -T ./src/tests_and_benchmarks/generate_tests_and_benchmarks.ts",
    "test:keep-snapshots": "npx jest",
    "test:run": "npx jest --updateSnapshot",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "typecheck": "tsc --noEmit",
    "benchmark:faker": "ts-node -T ./src/benchmarking_with_faker/benchmark_fake_data.ts",
    "faker:generate": "ts-node -T ./src/benchmarking_with_faker/generate_fake_data.ts",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "build": "rm -rf dist && tsc &&  cp -v src/generator/templates/*.handlebars dist/generator/templates/ && chmod +x ./dist/cli.js"
  },
  "bin": {
    "reco": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pita/reco.git"
  },
  "keywords": [
    "regex",
    "compile",
    "regular",
    "expression"
  ],
  "author": "Peter 'Pita' Martischka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pita/reco/issues"
  },
  "homepage": "https://github.com/Pita/reco#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/faker": "^5.1.5",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.13",
    "@types/rimraf": "^3.0.0",
    "@types/uglify-js": "^3.11.1",
    "@types/yargs": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-jest": "^26.6.3",
    "benchmark": "^2.1.4",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "faker": "^5.1.0",
    "fast-safe-stringify": "^2.0.7",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "handlebars": "^4.7.6",
    "js-string-escape": "^1.0.1",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "regenerate-unicode-properties": "^8.2.0",
    "regexpp": "^3.1.0",
    "uglify-js": "^3.12.1",
    "unicode-match-property-value-ecmascript": "^1.2.0",
    "yargs": "^16.2.0"
  }
}
