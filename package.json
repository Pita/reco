{
  "name": "reco.io",
  "version": "0.4.4",
  "description": "Regular Expression COmpiler",
  "main": "./dist/api.js",
  "types": "./dist/api.d.ts",
  "scripts": {
    "test:all": "npm run generate && npm run test:run",
    "generate": "node --require ts-node/register/transpile-only ./src/tests_and_benchmarks/generate_tests_and_benchmarks.ts",
    "generate:prof": "rm isolate*.txt; node --require ts-node/register/transpile-only --prof ./src/tests_and_benchmarks/generate_tests_and_benchmarks.ts; node --prof-process isolate*",
    "test:ci": "npx jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "typecheck": "tsc --noEmit",
    "benchmark": "cd lambda_benchmarking/ && npm run deploy:function && npm run benchmark",
    "benchmark:faker": "ts-node -T ./src/benchmarking_with_faker/benchmark_fake_data.ts",
    "faker:generate": "ts-node -T ./src/benchmarking_with_faker/generate_fake_data.ts",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "build": "rm -rf dist && tsc && chmod +x ./dist/cli.js",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "bin": {
    "reco.io": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pita/reco.git"
  },
  "keywords": [
    "regex",
    "compile",
    "regular",
    "expression"
  ],
  "author": "Peter 'Pita' Martischka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pita/reco/issues"
  },
  "homepage": "https://github.com/Pita/reco#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "@types/faker": "^5.1.7",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.32",
    "@types/prettier": "^2.2.2",
    "@types/rimraf": "^3.0.0",
    "@types/uglify-js": "^3.13.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "benchmark": "^2.1.4",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.4.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/standalone": "^7.13.9",
    "handlebars": "^4.7.7",
    "js-string-escape": "^1.0.1",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "regenerate-unicode-properties": "^8.2.0",
    "regexpp": "^3.1.0",
    "unicode-match-property-value-ecmascript": "^1.2.0",
    "yargs": "^16.2.0"
  }
}
